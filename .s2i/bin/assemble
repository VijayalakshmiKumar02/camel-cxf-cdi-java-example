#!/bin/bash

# install AppDynamics agent

curl -H "Accept: application/zip" https://raw.githubusercontent.com/benemon/fis-java-appdynamics-plugin/master/appdynamics-agent.zip -o /deployments/appdynamics-agent.zip 

pushd /deployments
unzip appdynamics-agent.zip
rm -f appdynamics-agent.zip
popd

# run original assemble script
/usr/local/s2i/assemble
